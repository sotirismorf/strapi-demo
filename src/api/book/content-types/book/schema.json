{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "yearPublished": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "books"
    },
    "file": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "quantities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quantity.quantity",
      "mappedBy": "book"
    },
    "publisher": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::publisher.publisher",
      "inversedBy": "books"
    }
  }
}
